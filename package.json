{
  "name": "graphql-osm",
  "version": "0.0.1",
  "description": "graphql object to schema mapping",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node_modules/.bin/nodemon --inspect src/index.ts --watch src/main --exec node_modules/.bin/ts-node src/index.ts",
    "start": "node_modules/.bin/ts-node src/main/server.ts",
    "ts:build": "tsc",
    "clean": "rimraf build",
