    "apollo",
    "schema",
    "mapping"
  ],
  "author": "Goran NikoliÄ‡ <goran.nikolic@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pfaze/graphql-osm/issues"
  },
  "homepage": "https://github.com/pfaze/graphql-osm#readme",
  "devDependencies": {
    "@types/node": "^10.5.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/winston": "^2.3.9",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "apollo-server": "^2.0.0-rc.6",
    "graphql": "^0.13.2",
